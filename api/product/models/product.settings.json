  "connection": "default",
  "collectionName": "products",
  "info": {
    "name": "Product"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Subtitle": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "Link": {
      "type": "string"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "Gallery": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "category": {
      "model": "category"
    }
  }
}
